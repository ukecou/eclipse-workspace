		 	int[] AA = new int[] {75, 80, 95, 100, 65, 50, 10, 0, 99};
		 	int HH = 0;
		 	
		 for(int B = 0; B < AA.length; B++) {
			 
			if(AA[B] > HH) {
			  HH = AA[B];
			 
			  }
			}
		 System.out.println("最大"+ HH);
		 
		int[] min = new int[]  {75, 80, 95, 100, 65, 50, 10, 0, 99};
		int EE = 100;
		
		for(int i = 0; i < min.length; i++) {
		 if(min[i] < EE) {
			 EE = min[i]; 
		 }
		}
			System.out.println("最小"+ EE);
			
		int[] ave = new int[] {	75, 80, 95, 100, 65, 50, 10, 0, 99};
		int sum =0;
		
		for(int i = 0; i < ave.length; i++ ) {
			sum += ave[i] ;
		}
		int AAAA = sum / ave[i];
		System.out.println("平均"+ AAAA);
			
			
 }
