package jp.co.seraku.serabank;

public class Account {

	int code;
	int amount;
	String ownername;

	void deposit(int value) {
		amount += value;
	}

	void withdraw(int value) {
		if (amount >= value) {
			amount -= value;
		} else {
			System.out.println("koetemasu");
		}
		
		
	}

	void transher(Account destAccount, int value) {
		if(amount >= value) {
			this.withdraw(value);
			destAccount.deposit(value);
		}
		
	}
	
	
}

